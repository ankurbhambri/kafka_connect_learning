curl -X POST -H "Content-Type: application/json" --data '{
    "name": "file-stream-demo-distributed",
    "config": {"connector.class":"FileStreamSourceConnector",
    "tasks.max":"1",
    "file":"demo-file.txt",
    "topics":"demo-1-distributed"
    }
}' http://localhost:8083/connectors


curl -X POST -H "Content-Type: application/json" --data '{
